{
	"presets": [
		["@babel/preset-typescript", { "onlyRemoveTypeImports": true }],
		[
			"@babel/preset-react",
			{
				"pragma": "h",
				"pragmaFrag": "Fragment"
			}
		]
	],
	"plugins": [
		"transform-functional-return",
		"@babel/plugin-proposal-optional-chaining",
		"@babel/plugin-transform-react-jsx"
	]
}
